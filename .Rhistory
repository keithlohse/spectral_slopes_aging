anova(mod1)
### Central Channel
mod2<- lm(Exponent ~ 1+group, data = Central)
anova(mod2)
### Parietal Channel
mod3<- lm(Exponent ~ 1+group, data = Parietal)
anova(mod3)
### Occipital Channel
mod4<- lm(Exponent ~ 1+ group, data = Occipital)
anova(mod4)
# Age-related differences in Intercepts
